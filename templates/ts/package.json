{
  "name": "react-templates",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "genprotos": "cross-env REPO_DIR=./ ./scripts/proto/gen_protos.sh",
    "postinstall": "yarn genprotos",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "start": "NODE_OPTIONS='--max-old-space-size=4086' && webpack serve --config build/webpack.config.dev.js",
    "build": "webpack build --config build/webpack.config.prod.js",
    "build:preview": "yarn build && serve dist -p 4001 -s",
    "build:analyze": "cross-env ANALYZE=true webpack build --config build/webpack.config.prod.js",
    "lint:script": "prettier --check --write src/**/*.* && eslint --fix src/**/*.*",
    "lint:style": "prettier --check --write src/**/*.* && stylelint src/**/*.scss --syntax=scss --fix && stylelint src/**/*.css --syntax=css --fix"
  },
  "lint-staged": {
    "src/**/*.js|src/**/*.jsx|src/**/*.ts|src/**/*.tsx|src/**/*.json": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "src/**/*.scss": [
      "prettier --write",
      "stylelint --syntax=scss --fix",
      "git add"
    ],
    "src/**/*.css": [
      "prettier --write",
      "stylelint --syntax=css --fix",
      "git add"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/plugin-syntax-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.16.0",
    "@babel/runtime-corejs3": "^7.16.0",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-config": "^5.0.3",
    "@types/react-router-dom": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autoprefixer": "^10.3.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "cache-loader": "^4.1.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "css-modules-typescript-loader": "^4.0.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^6.3.4",
    "google-protobuf": "^3.18.0-rc.2",
    "html-webpack-plugin": "^5.3.2",
    "http-server": "^13.0.2",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "mini-css-extract-plugin": "^2.3.0",
    "node-sass": "^6.0.1",
    "portfinder-sync": "^0.0.2",
    "postcss": "^8.3.8",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.4.1",
    "protoc-gen-grpc-web": "1.2.1",
    "regenerator-runtime": "^0.13.9",
    "sass-loader": "^12.1.0",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.2.1",
    "stylelint": "^13.13.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.21.0",
    "stylelint-webpack-plugin": "^3.0.1",
    "terser-webpack-plugin": "^5.2.4",
    "ts-loader": "^9.2.6",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.4.4",
    "webpack": "^5.51.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.0.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "grpc-web": "1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0"
  }
}
